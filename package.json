{
    "name": "hipsum",
    "displayName": "hipsum",
    "description": "Inserts hipster placeholder text (lorem ipsum) from http://hipsterjesus.com/ into the editor.",
    "version": "1.0.6",
    "publisher": "third774",
    "engines": {
        "vscode": "^1.0.1"
    },
    "bugs": {
        "url": "https://github.com/third774/vscode-hipsum/issues"
    },
    "scripts": {
        "test": "mocha"
    },
    "homepage": "https://github.com/third774/vscode-hipsum",
    "repository": {
        "type": "git",
        "url": "https://github.com/third774/vscode-hipsum"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.hipsum"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.hipsum",
                "title": "hipsum"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}
